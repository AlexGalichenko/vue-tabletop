{
  "name": "vue-tabletop-socketio",
  "version": "0.0.1",
  "description": "vue tabletop simulator",
  "scripts": {
    "webpack:client": "webpack --mode production --config webpack.conf.js",
    "webpack:server": "webpack --mode production --config webpack.server.conf.js",
    "webpack:watch:client": "webpack --mode development --config webpack.conf.js --watch",
    "webpack:watch:server": "webpack --mode development --config webpack.server.conf.js",
    "preserver": "webpack --mode development --config webpack.server.conf.js",
    "server": "node dist/server.js",

    "start": "npm run server",
    "prestart": "npm run webpack:server",
    "preprestart": "npm run webpack:client"
  },
  "keywords": [
    "tabletop",
    "simulator"
  ],
  "author": "Alexandr Galichenko",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "express": "^4.17.1",
    "interactjs": "^1.9.19",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.11",
    "vue-material": "^1.0.0-beta-14",
    "vue-router": "^3.3.4",
    "vuex": "^3.4.0",

    "@babel/core": "^7.9.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^4.3.0",
    "style-loader": "^1.1.1",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "latest"
  }
}
